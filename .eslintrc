{
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "plugins": ["jest"],
  "root": true,
  "globals": {
  },
  "env": {
      "jest": true
  },
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/jsx-props-no-spreading": 0,
  }
}
